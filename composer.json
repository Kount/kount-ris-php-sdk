{
  "name": "kount/kount-ris-php-sdk",
  "description": "PHP client for Kount Risk Inquiry Service.",
  "keywords": [
    "test",
    "payments",
    "sdk"
  ],
  "homepage": "https://github.com/Kount/kount-ris-php-sdk",
  "type": "library",
  "license": "proprietary",
  "authors": [
    {
      "name": "Kount",
      "email": "support@kount.com"
    }
  ],
  "require": {
    "php": ">=8.2.25",
    "ext-curl": "*",
    "ext-json": "*",
    "ext-mbstring": "*",
    "ext-xml": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^11.5.12",
    "phpunit/php-code-coverage": "^11.0.9"
  },
  "autoload": {
    "classmap": [
      "./src/"
    ]
  },
  "config": {
    "allow-plugins": {
      "symfony/flex": true
    }
  }
}
